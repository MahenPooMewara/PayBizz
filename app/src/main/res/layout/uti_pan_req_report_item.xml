<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:orientation="vertical"
    android:background="@color/pay_bizz_orange"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/serial_no_id"
        style="@style/PBReportTableTV"
        android:text="@string/serial_no"/>
    <TextView
        android:id="@+id/no_of_coupon"
        style="@style/PBReportTableTV"
        android:text="@string/no_of_coupon"/>
    <TextView
        android:id="@+id/amt"
        style="@style/PBReportTableTV"
        android:text="@string/amt"/>
    <TextView
        android:id="@+id/date_time"
        style="@style/PBReportTableTV"
        android:text="@string/date_time"/>
    <TextView
        android:id="@+id/status_id"
        style="@style/PBReportTableTV"
        android:text="@string/status"/>
    <TextView
        android:id="@+id/remark"
        style="@style/PBReportTableTV"
        android:text="@string/remark"/>
    <TextView
        android:id="@+id/action_date"
        style="@style/PBReportTableTV"
        android:text="@string/action_date"/>

</LinearLayout>